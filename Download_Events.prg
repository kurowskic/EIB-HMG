*-----------------------------------------------------------------------------*
PROCEDURE win_Download_OnInit()
*-----------------------------------------------------------------------------*

  CTK_DrawBorder( "win_Download" )

  DO_Events()

  download_csv()
  
  DO_Events()

  aFrm := CTK_Release( aFrm )

  win_Download.Release

RETURN
*-----------------------------------------------------------------------------*


*-----------------------------------------------------------------------------*
PROCEDURE win_Download_OnGotFocus()
*-----------------------------------------------------------------------------*

  CenterModalWindow( "win_Download" )

  AutoAdjustControls( "win_Download" )

  CTK_DrawBorder( "win_Download" )

RETURN
