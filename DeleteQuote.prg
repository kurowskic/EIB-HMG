FUNCTION DeleteQuote( xcString )
*-----------------------------------------------------------------------------*

  xcString := ALLTRIM( STRTRAN( xcString , CHR( 34 ) , "" ) )

  xcString := STRTRAN( xcString , CHR( 34 ) + CHR( 34 ) + CHR( 34 ) , "" )
  xcString := STRTRAN( xcString , CHR( 34 ) + CHR( 34 ) , "" )
  xcString := STRTRAN( xcString , CHR( 34 ) , "" )

  xcString := STRTRAN( xcString , CHR( 160 ) , CHR ( 32 ) )

  xcString := STRTRAN( xcString , CHR( 32 ) + CHR ( 32 ) , CHR ( 32 ) )

  xcString := ALLTRIM( xcString )

RETURN xcString
