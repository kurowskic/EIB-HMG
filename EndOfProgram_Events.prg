*-----------------------------------------------------------------------------*
PROCEDURE EndOfProgram_btn_YES()
*-----------------------------------------------------------------------------*

  RELEASE WINDOW ALL

RETURN
*-----------------------------------------------------------------------------*


*-----------------------------------------------------------------------------*
PROCEDURE EndOfProgram_btn_NOT()
*-----------------------------------------------------------------------------*

  ViewData( nPage )


  aFrm := CTK_Release( aFrm )

  ThisWindow.Release


RETURN
*-----------------------------------------------------------------------------*


*-----------------------------------------------------------------------------*
PROCEDURE EndOfProgram_OnInit()
*-----------------------------------------------------------------------------*

  CTK_DrawBorder( "win_EndOfProgram" )

  DO_Events()

RETURN
*-----------------------------------------------------------------------------*


*-----------------------------------------------------------------------------*
PROCEDURE EndOfProgram_OnGotFocus()
*-----------------------------------------------------------------------------*

  afrm := CTK_Restore( aFrm )
 
RETURN
*-----------------------------------------------------------------------------*


